<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="540.0" prefWidth="777.0" styleClass="parent" stylesheets="@../../Style/Style.css" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.aptech.eproject2_prosmiles.Controller.PrescriptionListController">
    <children>
        <TableView fx:id="tablePrescription" layoutY="52.0" prefHeight="488.0" prefWidth="777.0">
            <columns>
                <TableColumn fx:id="colId" prefWidth="40.0" text="ID" />
                <TableColumn fx:id="colPatientName" prefWidth="175.0" text="Patient's Name" />
                <TableColumn fx:id="colDescription" prefWidth="307.0" text="Description" />
                <TableColumn fx:id="colCreatedAt" prefWidth="133.0" text="Created Time" />
                <TableColumn fx:id="colStatus" prefWidth="102.0" text="Status" />
            </columns>
        </TableView>
        <Button fx:id="btnAddNew" layoutX="588.0" layoutY="14.0" mnemonicParsing="false" text="Add New Prescription">
            <styleClass>
                <String fx:value="master_button" />
                <String fx:value="title" />
            </styleClass>
        </Button>
        <Label layoutX="14.0" layoutY="19.0" styleClass="title" text="Prescription's List" />
        <Button fx:id="btnDelete" layoutX="425.0" layoutY="14.0" mnemonicParsing="false" text="Delete Prescription">
            <styleClass>
                <String fx:value="button-delete" />
                <String fx:value="title" />
            </styleClass>
        </Button>
    </children>
</AnchorPane>
